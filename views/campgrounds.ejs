<%- include("partials/header") %>

<div class="container">
    <header class="jumbotron">
        <div class="container">
            <h1 class="display-4">Welcome to YelpCamp</h1>
            <p class="lead">View our hand-picked campgrounds from all over the world!</p>
            <p>
                <a class="btn btn-primary btn-lg" href="/campgrounds/new">Add new Campground</a>
            </p>
        </div>
    </header>
    
    
    <% campgrounds.forEach(function(campground) { %>
    <div>
        <h4><%= campground.name %></h4>
        <img src="<%= campground.image %> " alt="">
    </div>
    <% }) %>
</div>

<%- include("partials/footer") %>
